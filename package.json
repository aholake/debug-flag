{
  "version": "0.1.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/aholake/debug-flag"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "debug-flag",
  "author": "Loc Vo",
  "description": "Debug flag component for React",
  "module": "dist/debug-flag.esm.js",
  "size-limit": [
    {
      "path": "dist/debug-flag.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/debug-flag.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "husky": "^6.0.0",
    "postcss": "^8.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.5",
    "rollup-plugin-postcss": "^4.0.0"
  }
}
